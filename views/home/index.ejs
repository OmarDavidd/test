<h4>Hola a mi home</h4>
<div>
	<h6>Lista de productos</h6>
	<table>
		<thead>
			<tr>
				<th>Nombre</th>
				<th>Precio</th>
			</tr>
		</thead>
		<tbody>
			<% ps.forEach(p => { %>
				<tr>
					<td><%= p.nombre %></td>
					<td><%= p.precio %></td>
				</tr>
			<% }); %>
		</tbody>

	</table>
</div>
